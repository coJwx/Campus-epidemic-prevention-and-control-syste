,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAIA,oBAAA,MAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA;AACA;AACA;AAvBA,aAAA;AAyBA;AAEA;AAGA;AACA,KA1EA;;AA2EA;;;AAGA,IAAA,IA9EA,kBA8EA;AAAA;;AACA,WAAA,QAAA,CAAA,eAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA;AACA,OANA,EAMA,KANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAXA;AAYA,KA3FA;AA4FA,IAAA,KA5FA,iBA4FA,CA5FA,EA4FA;AACA,WAAA,QAAA,CAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA;AAjGA;AAxCA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-row>\n      <el-col :span=\"24\">\n        <el-card>\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align:center\">\n            <span>健康信息上报</span>\n          </div>\n          <el-table\n            :data=\"tableData\"\n            border\n            style=\"width: 100%;\">\n            <el-table-column\n              prop=\"username\"\n              label=\"姓名\">\n            </el-table-column>\n            <el-table-column\n              prop=\"classes\"\n              label=\"班级\">\n            </el-table-column>\n            <el-table-column\n              prop=\"address\"\n              label=\"生源地\">\n            </el-table-column>\n          </el-table>\n        </el-card>\n        <el-divider></el-divider>\n\n      <el-card v-if=\"show==false\">\n          <div slot=\"header\" style=\"text-align: center\">\n              <span>今日检测情况</span>\n          </div>\n          <el-alert\n                  title=\"今日申请表提交成功\"\n                  type=\"success\"\n                  center\n                  show-icon>\n          </el-alert>\n          <el-table\n                  :data=\"tableData1\"\n                  border\n                  style=\"width: 100%\">\n              <el-table-column\n                      prop=\"temperature\"\n                      label=\"当前体温\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"hot\"\n                      label=\"是否发热、咳嗽\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"fever\"\n                      label=\"是否有去就诊\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"gohubei\"\n                      label=\"是否去过高危地区\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"hubeiren\"\n                      label=\"是否为高危地区人员\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"mask\"\n                      label=\"是否接触过高危地区人员\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"leaveout\"\n                      label=\"是否离开过学校\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"hesuan\"\n                      label=\"是否做过核酸检测\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"masknum\"\n                      label=\"口罩数量\">\n              </el-table-column>\n              <el-table-column\n                      prop=\"kills\"\n                      label=\"杀毒用品是否充足\">\n              </el-table-column>\n          </el-table>\n\n      </el-card>\n\n        <el-card v-else-if=\"show==true\">\n          <el-form ref=\"form\" :model=\"form\" label-width=\"150px\">\n                <el-form-item label=\"当前体温\">\n                    <el-input v-model=\"form.temp\" style=\"width:130px\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"是否发热、咳嗽\">\n                    <el-radio-group v-model=\"form.radio1\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"是否有去就诊\">\n                    <el-radio-group v-model=\"form.radio2\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"是否去过高危地区\">\n                    <el-radio-group v-model=\"form.radio3\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n              <el-form-item label=\"是否为高危地区人员\">\n                  <el-radio-group v-model=\"form.radio9\">\n                      <el-radio label=\"否\"></el-radio>\n                      <el-radio label=\"是\"></el-radio>\n                  </el-radio-group>\n              </el-form-item>\n                <el-form-item label=\"是否接触过高危地区人员\">\n                    <el-radio-group v-model=\"form.radio7\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"是否离开过学校\">\n                    <el-radio-group v-model=\"form.radio5\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"是否做过核酸检测\">\n                    <el-radio-group v-model=\"form.radio6\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"口罩数量\">\n                    <el-input v-model=\"form.number\" style=\"width:130px\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"杀毒用品是否充足\">\n                    <el-radio-group v-model=\"form.radio8\">\n                    <el-radio label=\"否\"></el-radio>\n                    <el-radio label=\"是\"></el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"primary\" @click=\"open\">提交</el-button>\n                </el-form-item>\n                </el-form>\n        </el-card>\n\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n      return {\n          show:true,\n        tableData: [],\n          tableData1: [],\n        form: {\n          temp: '',\n          number:'',\n          radio1: '',\n          radio2: '',\n          radio3: '',\n          radio4: '',\n          radio5: '',\n          radio6: '',\n          radio7: '',\n          radio8: '',\n            radio9: '',\n        }\n      }\n    },\n    created(){\n        /**根据token获取当前用户信息\n         * this.tableData列表数据\n         * */\n        this.$axiosjwt({\n            url:\"/users/getUserDataByToken\",\n            method:\"get\",\n            data:{},\n            success:(result)=>{\n                this.tableData.push(result)\n            }\n        })\n\n        /**\n         *\n         *调用获取当天某用户报表并且决定是否允许填写\n         */\n        this.getnowdatah()\n    },\n     methods: {\n         /**\n          *   //获取当天某用户报表并且决定是否允许填写\n          */\n      getnowdatah(){\n          this.$axiosjwt({\n              url:'/students/getHealthNowDayByid',\n              method:\"get\",\n              data:{},\n              success:(result)=> {\n                //   console.log(result,\"xxxxxxxxxxxxx这是结果!!!!!!!!!!!!!!!!!!!!!---------------------\")\n                  if(result.length==0) this.show=true\n                  else {\n                      this.show=false;\n                      this.tableData1.push(result[0])\n                  }\n              }\n          });\n      },/**\n          健康表提交\n          temperature:this.form.temp,\n          hot:this.form.radio1,\n          gohubei:this.form.radio3,\n          hubeiren:this.form.radio9,\n          leave:this.form.radio5,\n          hesuan:this.form.radio6,\n          mask:this.form.radio7,\n          masknum:this.form.number,\n          kill:this.form.radio8,\n          fever:this.form.radio2,\n          */\n         onSubmit() {\n             if (!this.form.temp || !this.form.radio1 || !this.form.radio3 || !this.form.radio9 || !this.form.radio5 || !this.form.radio6 ||!this.form.radio7 ||!this.form.number || !this.form.radio8 || !this.form.radio2)\n                this.open3(\"填报表不可留空\")\n             else{\n                 let reg=/^\\d+(\\.\\d+)?$/\n                 let reg1=/^(([0-9]+\\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\\.[0-9]+)|([0-9]*[1-9][0-9]*))$/\n                 let tempBool=reg1.test(this.form.temp)\n                 let   mskBool=reg.test(this.form.number)\n                 if (!tempBool) this.open3(\"体温请输入数字\")\n                 else{\n                     if (!mskBool)  this.open3(\"口罩数量请用数字方式输入\")\n                     else{\n                         this.$axiosjwt({\n                             url:\"/students/sethealth\",\n                             method:\"post\",\n                             data:{\n                                 temperature:this.form.temp,\n                                 hot:this.form.radio1,\n                                 gohubei:this.form.radio3,\n                                 hubeiren:this.form.radio9,\n                                 leave:this.form.radio5,\n                                 hesuan:this.form.radio6,\n                                 mask:this.form.radio7,\n                                 masknum:this.form.number,\n                                 kill:this.form.radio8,\n                                 fever:this.form.radio2,\n                             },\n                             success:(result)=>{\n                                 this.$message({\n                                     type:'success',\n                                     message:\"提交成功\"\n                                 })\n                                 if (result.length!=0){\n                                     this.getnowdatah()\n                                 }\n                             }\n                         })\n                     }\n                     \n                 }\n\n\n             }\n         },\n         /**\n          * 提交弹窗\n          */\n         open() {\n             this.$confirm('提交健康报表, 是否继续?', '提示', {\n                 confirmButtonText: '确定',\n                 cancelButtonText: '取消',\n                 type: 'warning'\n             }).then(() => {\n                 this.onSubmit()\n             }).catch(() => {\n                 this.$message({\n                     type: 'info',\n                     message: '已取消提交'\n                 });\n             });\n         },\n         open3(v) {\n             this.$message({\n                 message:v,\n                 type: 'warning'\n             });\n         },\n\n    },\n}\n</script>\n\n<style scoped lang=\"scss\">\n\n</style>\n"],"sourceRoot":"src/components/main"}]}